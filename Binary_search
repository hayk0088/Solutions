def binary_search(start,end,sorted_numbers,num):
    middle = (start + end) // 2 
    middle_1=sorted_numbers[middle]
    if start <= end:
        if  middle_1 == num : 
            return middle
        if  middle_1 > num:
            return binary_search(start,middle-1,sorted_numbers,num)
        else:
            return binary_search(middle + 1,end,sorted_numbers,num)
    return -1
 
sorted_numbers = [1,5,8,15,26,48,85,165,267,459,568,679,790,1000]
num = int(input())
print(binary_search(0,len(sorted_numbers)-1,sorted_numbers,num))
